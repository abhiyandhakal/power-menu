<schemalist>

  <schema id="org.gnome.gthumb.slideshow" path="/org/gnome/gthumb/slideshow/" gettext-domain="gthumb">
    <key name="change-delay" type="d">
      <default>5.0</default>
    </key>
    <key name="wrap-around" type="b">
      <default>false</default>
    </key>
    <key name="automatic" type="b">
      <default>true</default>
    </key>
    <key name="transition" type="s">
      <default>'fade-in'</default>
    </key>
    <key name="random-order" type="b">
      <default>false</default>
    </key>
  </schema>

</schemalist>
