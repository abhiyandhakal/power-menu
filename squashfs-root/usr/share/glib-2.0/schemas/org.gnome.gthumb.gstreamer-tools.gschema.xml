<schemalist>

  <schema id="org.gnome.gthumb.gstreamer-tools" path="/org/gnome/gthumb/gstreamer-tools/" gettext-domain="gthumb">
    <key name="screenshot-location" type="s">
      <default>'file://~'</default>
    </key>
    <key name="volume" type="i">
      <default>100</default>
    </key>
    <key name="mute" type="b">
      <default>false</default>
    </key>
    <key name="use-hardware-acceleration" type="b">
      <default>false</default>
    </key>
    <key name="zoom-to-fit" type="b">
      <default>false</default>
    </key>
  </schema>

</schemalist>
